{
  "name": "create-kkt",
  "version": "2.0.0",
  "description": "Creates a KKT application using the command line.",
  "homepage": "https://github.com/kktjs/create-kkt",
  "author": "Kenny Wong <wowohoo@qq.com> (https://github.com/jaywcjlove)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kktjs/create-kkt"
  },
  "typings": "lib/create.d.ts",
  "bin": {
    "create-kkt": "lib/cli.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "start": "node lib/index.js",
    "watch": "npm run watch:ts",
    "watch:ts": "tsbb watch & npm run build:types -- --watch",
    "build": "npm run build:ts && npm run build:types",
    "build:ts": "tsbb build",
    "build:types": "tsbb types",
    "test": "tsbb test",
    "prettier": "prettier --write \"**/*.{js,ts,less,md,json}\"",
    "coverage": "tsbb test --coverage"
  },
  "files": [
    "lib",
    "src"
  ],
  "jest": {},
  "keywords": [
    "kkt",
    "react",
    "redux",
    "rematch",
    "uiw",
    "redux-saga",
    "elm",
    "framework",
    "frontend"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/download": "6.2.4",
    "@types/fs-extra": "9.0.6",
    "@types/minimist": "1.2.1",
    "@types/node": "14.14.20",
    "@types/yargs": "13.0.8",
    "husky": "4.3.7",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "tsbb": "1.7.9"
  },
  "dependencies": {
    "@babel/runtime": "7.9.2",
    "ora": "5.2.0",
    "download": "8.0.0",
    "minimist": "1.2.5",
    "fs-extra": "9.0.1"
  }
}
